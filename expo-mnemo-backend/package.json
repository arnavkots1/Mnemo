{
  "name": "mnemo-backend",
  "version": "1.0.0",
  "description": "Backend API stub for Mnemo emotion classification",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:tunnel": "cross-env USE_TUNNEL=true ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "watch": "tsc --watch"
  },
  "keywords": [
    "mnemo",
    "emotion",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
