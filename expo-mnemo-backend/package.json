    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "expo": "~54.0.27",
    "expo-asset": "~12.0.11",
    "expo-av": "~16.0.8",
    "expo-file-system": "~19.0.20",
    "expo-image-picker": "~17.0.9",
    "expo-location": "~19.0.8",
    "expo-sensors": "~15.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-task-manager": "~14.0.9",
    "express": "^4.18.2",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/localtunnel": "^2.0.4",
    "@types/node": "^20.10.0",
    "cross-env": "^10.1.0",
